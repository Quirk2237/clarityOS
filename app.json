{
  "expo": {
    "name": "ClarityOS",
    "slug": "canopyOS",
    "scheme": "canopyos",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": ["ios", "android", "web"],
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      },
      "bundleIdentifier": "com.clarityos.radianthillsoftware"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png"
      },
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      },
      "package": "com.clarityos.radianthillsoftware",
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "buildToolsVersion": "34.0.0",
      "kotlinVersion": "1.9.22",
      "versionCode": 1,
      "config": {
        "gradle": {
          "useDeprecatedNdk": false
        }
      }
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      "expo-updates"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "342ac03e-fa4d-4581-8bbd-f522dd7b5adc"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png",
      "name": "ClarityOS",
      "shortName": "ClarityOS",
      "lang": "en",
      "scope": "/",
      "themeColor": "#ffffff",
      "backgroundColor": "#ffffff",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/"
    },
    "owner": "untrue6165"
  }
}
